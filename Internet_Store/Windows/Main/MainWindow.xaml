<Window x:Class="Internet_Store.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Internet_Store"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <StackPanel VerticalAlignment="Center">
        <Button x:Name="Customer" Content="За покупками?"/>
        <Button x:Name="Seller" Content="Выложить товар?"/>
        <Button x:Name="Admin" Content="Админиистрировать?"/>
        </StackPanel>
    </Grid>
</Window>
